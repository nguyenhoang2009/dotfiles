\set QUIET 1
\pset null '[NULL]'

\set PROMPT1 '%[%033[1;31m%]%M%[%033[0m%]: %[%033[1;32m%]%n%[%033[0m%] -on- %[%033[1;35m%]%/%R%[%033[0m%]%# '
\set PROMPT2 '[more] %R > '

\timing

\x auto
\set VERBOSITY verbose
\set HISTFILE ~/.psql_history- :DBNAME
\set HISTCONTROL ignoredups
\set COMP_KEYWORD_CASE upper
\unset QUIET

