
"xbindkeys_show" 
  control+shift + q

# set directly keycode (here control + f with my keyboard)
"maim -s ~/screenshot/$(date +%F-%T).png"
    m:0x1 + c:107
    Shift + Print
"urxvt"
    m:0x1c + c:36
    Control+Alt+Mod2 + Return

# specify a mouse button
"terminology"
  control + b:2

